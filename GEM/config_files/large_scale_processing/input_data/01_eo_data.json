{
  "pipeline": "eogrow.pipelines.download_batch.BatchDownloadPipeline",
  "**global_config": "${config_path}/../global_config.json",
  "output_folder_key": "tiffs",
  "inputs": [
        {
            "data_collection": "BYOC_484d8dbb-9e3e-41f2-b96b-35189d3ae37f", // 120m global mosaic, https://collections.sentinel-hub.com/sentinel-s2-l2a-mosaic-120/
            "resampling_type": "BILINEAR",
            "mosaicking_order": "leastRecent"
        }
  ],
  "evalscript_path": "${import_path:gem_example}/../config_files/input_data/evalscript.js",  //this resolves paths correctly on cluster instances
  "tiff_outputs": ["B01", "B02", "B03", "B04", "B05", "B06", "B07", "B08", "B8A", "B09", "B11", "B12", "QM"],
  "save_userdata": true,
  "monitoring_sleep_time": 60,
  "batch_output_kwargs":{
    "overwrite": true,
    "skipExisting": false
  },
  "analysis_only": false,
  "batch_id": "" // should be added after it is created
}